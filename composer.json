{
  "name": "aaronheath/laravel-oauth-client",
  "description": "A simple oauth client for use in my applications.",
  "license": "MIT",
  "keywords": ["laravel", "oauth"],
  "authors": [
    {
      "name": "Aaron Heath",
      "email": "aaron@aaronheath.com",
      "homepage": "https://aaronheath.com"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "illuminate/support": "^7.0",
    "guzzlehttp/guzzle": "^6.5"
  },
  "autoload": {
    "psr-4": {
      "Heath\\OauthClient\\": "src/"
    }
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "~8.0",
    "orchestra/testbench": "^5.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Heath\\OauthClient\\OauthClientServiceProvider"
      ]
    }
  }
}
